//Create a composition with a series of lines and ellipses. Use an if structure to select 
// which lines of code to run and which to skip
size (100,100);
background (0);
fill (255);
stroke (255);
if(10>12)
ellipse (50,50,50,50);
if (8>4)
line (0,0,100,100);
if ((8>7)&&(2>=2)){
  line (10,0,110,100);
}
if (10<13)
ellipse (80,30,20,20);
