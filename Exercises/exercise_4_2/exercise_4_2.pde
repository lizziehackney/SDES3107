//Use multiplication to create a series of lines with increasing space between each.
size (200,200);
background (255);
fill (0);
stroke (0);
int a=10;
// multiplying to make lines parallel
line (a,a*2,a,a*5);
line (a*2,a*2,a*2,a*5);
line (a*4,a*2,a*4,a*5);
line (a*8,a*2,a*8,a*5);
line (a*16,a*2,a*16,a*5);
